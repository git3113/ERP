{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SystemConfig": {
    "LoginProvider": "WebApi", // 登录信息保存方式 Cookie Session WebApi
    "SnowFlakeWorkerId": 2, // SnowFlake 节点序号
    "AllowCorsSite": "http://localhost:8000", // 允许的其他站点访问Api

    "DBProvider": "SqlServer",
    "DBConnectionString": "Server=.;Initial Catalog=webapp;User ID=sa;Password=123456",
    "DBCommandTimeout": 180, // 数据库超时时间，单位秒
    "DBBackup": "", // 数据库备份路径

    "CacheProvider": "Memory", // 缓存使用方式 Memory Redis
    "RedisConnectionString": "127.0.0.1:6379"
  }
}
